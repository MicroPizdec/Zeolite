{
  "name": "zeolite",
  "version": "4.2",
  "description": "Discord bot",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node --enable-source-maps .",
    "start:dev": "npx ts-node src/index",
    "prebuild": "node -p \"'export const version = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "tsc --build --verbose",
    "postbuild": "node scripts/generateBuildDate.js",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "author": "w3lcharm, alyzter",
  "license": "MIT",
  "devDependencies": {
    "@types/color-convert": "^2.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.11.12",
    "@types/qrcode": "^1.4.2",
    "@types/validator": "^13.7.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "canvas": "^2.10.2",
    "color-convert": "^2.0.1",
    "common-tags": "^1.8.2",
    "emoji-regex": "^10.0.0",
    "erela.js": "^2.4.0",
    "js-yaml": "^4.1.0",
    "log4js": "^6.7.1",
    "oceanic.js": "^1.4.0",
    "qrcode": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.11.0",
    "sequelize-typescript": "^2.1.1",
    "sqlite3": "^5.0.2",
    "zeolitecore": "github:MicroPizdec/ZeoliteCore"
  }
}
